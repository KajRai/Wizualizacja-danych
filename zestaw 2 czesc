import matplotlib.pyplot as plt
import pandas as pd
import numpy as np

nazwy = ["Hipermarkety","Supermarkety","Domy handlowe"]
w_2016 = [6,32,2]
w_2017 = [8, 26, 3.5]
X = np.arange(len(nazwy))
plt.bar(X,w_2016,width=0.25)
plt.bar(X + 0.25,w_2017,width=0.25)
plt.xticks(X + 0.125, nazwy )
plt.show()
